{
  "name": "com.ecsphysics2d.core",
  "version": "0.1.0",
  "displayName": "ECSPhysics2D",
  "description": "ECSPhysics2D is an ECS-compatible wrapper for the UnityEngine.LowLevelPhysics2D Physics engine. The physics engine is struct-based, multithreaded, high performance, and out of the box DOTS compatible.",
  "unity": "6000.3",
  "unityRelease": "0f1",
  "documentationUrl": "https://github.com/yohash/ECSPhysics2D/blob/main/README.md",
  "changelogUrl": "https://github.com/yohash/ECSPhysics2D/blob/main/Assets/ECSPhysics2D/CHANGELOG.md",
  "licensesUrl": "https://github.com/yohash/ECSPhysics2D/blob/main/Assets/ECSPhysics2D/LICENSE.md",
  "dependencies": { 
    "com.unity.entities": "1.4.4",
    "com.unity.burst": "1.8.27",
    "com.unity.collections": "2.6.4",
    "com.unity.mathematics": "1.3.3"
  },
  "keywords": [
    "ecs",
    "physics2d",
    "dots"
  ],
  "samples": [
    {
      "displayName": "Falling Shapes",
      "description": "Demonstrates shape creation and explosions.",
      "path": "Samples~/FallingShapes"
    },
    {
      "displayName": "Falling Cascade",
      "description": "Demonstrates collision event detection and collision layers.",
      "path": "Samples~/FallingCascade"
    },
    {
      "displayName": "Joints Breaking",
      "description": "Demonstrates hinge joints with springs, hinge joints with motors, weld joints, and joint collision.",
      "path": "Samples~/JointBreaking"
    },
    {
      "displayName": "Multiple Physics Worlds",
      "description": "Demonstrates two physics worlds with no interactions.",
      "path": "Samples~/MultiWorldDemo"
    }
  ],
  "author": {
    "name": "Jason Heebl",
